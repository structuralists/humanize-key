{
   "name": "humanize-key",
   "version": "0.2.5",
   "description": "formatting string keys for display",
   "keywords": [
      "humanize",
      "key",
      "string",
      "formatting"
   ],
   "main": "dist/index.js",
   "module": "dist/index.mjs",
   "types": "dist/index.d.ts",
   "author": "William Lubelski <will.lubelski@gmail.com>",
   "repository": {
      "type": "git",
      "url": "https://github.com/lubelski/humanize-key"
   },
   "license": "MIT",
   "scripts": {
      "change": "pnpm changeset",
      "build": "tsup src/humanizeKey.ts --format cjs,esm --dts",
      "ci": "turbo build lint",
      "lint": "tsc",
      "release": "turbo run publish",
      "publish": "changeset publish"
   },
   "jest": {
      "transform": {
         ".(ts|tsx)": "ts-jest"
      },
      "testEnvironment": "node",
      "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js"
      ],
      "coveragePathIgnorePatterns": [
         "/node_modules/",
         "/test/"
      ],
      "coverageThreshold": {
         "global": {
            "branches": 70,
            "functions": 70,
            "lines": 70,
            "statements": 70
         }
      },
      "collectCoverageFrom": [
         "src/*.{js,ts}"
      ]
   },
   "prettier": {
      "semi": false,
      "singleQuote": true,
      "tabWidth": 3,
      "trailingComma": "all",
      "useTabs": false,
      "arrowParens": "always"
   },
   "devDependencies": {
      "@changesets/cli": "^2.26.0",
      "@types/jest": "^29.4.0",
      "@types/node": "^18.15.0",
      "check-export-map": "^1.3.0",
      "jest": "^29.5.0",
      "tsx": "^3.12.3",
      "turbo": "^1.8.3",
      "typescript": "^4.9.5",
      "tsup": "^6.6.3"
   },
   "publishConfig": {
      "access": "public"
   }
}
